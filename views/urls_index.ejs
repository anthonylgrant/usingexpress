      <ul>
      <% for(let id in urls) {
        let url = urls[id]; %>
        <li>
          <a href="/urls/<%= id %>"><%= url %>/<%= id %>
          <a>

          <form method="POST" action="/urls/<%= id %>/delete">
          <input type="submit" value="Delete"></form>
        </li>
        <%
      }
      %>
    </ul>




    </header>
